{
  "name": "dalywinterllp",
  "version": "2.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host=0.0.0.0",
    "test": "ng test",
    "test:ci": "ng test --watch false --browsers ChromeHeadless",
    "lint": "ng lint",
    "lint:all": "ng lint --fix",
    "e2e": "ng e2e",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:ssr": "node dist/server",
    "build": "npm run lint:all && npm run build:ssr",
    "build:stats": "ng build --prod --stats-json && ng run dalywinterllp:server:production",
    "analyze": "webpack-bundle-analyzer dist/browser/stats-es2015.json",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
    "build:client-and-server-bundles": "ng build --prod && ng run dalywinterllp:server:production",
    "test:coverage": "npm run test:ci -- --code-coverage",
    "next-update": "next-update -k true --tldr"
  },
  "private": true,
  "greenkeeper": {
    "ignore": [
      "typescript"
    ]
  },
  "dependencies": {
    "@angular/animations": "^8.2.0",
    "@angular/cdk": "^8.1.2",
    "@angular/common": "^8.2.0",
    "@angular/compiler": "^8.2.0",
    "@angular/core": "^8.2.0",
    "@angular/fire": "^5.2.1",
    "@angular/flex-layout": "^8.0.0-beta.26",
    "@angular/forms": "^8.2.0",
    "@angular/material": "^8.1.2",
    "@angular/platform-browser": "^8.2.0",
    "@angular/platform-browser-dynamic": "^8.2.0",
    "@angular/platform-server": "^8.2.0",
    "@angular/pwa": "^0.803.0",
    "@angular/router": "^8.2.0",
    "@angular/service-worker": "^8.2.0",
    "@nguniversal/express-engine": "8.1.1",
    "@nguniversal/module-map-ngfactory-loader": "8.1.1",
    "express": "^4.15.2",
    "firebase": "^6.4.2",
    "hammerjs": "^2.0.8",
    "ngx-toastr": "^10.0.4",
    "rxjs": "~6.5.2",
    "zone.js": "~0.10.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.0",
    "@angular/cli": "^8.2.1",
    "@angular/compiler-cli": "^8.2.0",
    "@angular/language-service": "^8.2.0",
    "@types/jasmine": "~3.4.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~13.7.0",
    "codacy-coverage": "^3.4.0",
    "codecov": "^3.5.0",
    "codelyzer": "^5.0.1",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "next-update": "^3.6.0",
    "protractor": "~5.4.0",
    "ts-loader": "^6.0.4",
    "ts-node": "~8.3.0",
    "tslib": "^1.10.0",
    "tslint": "~5.18.0",
    "typescript": "~3.4.5",
    "webpack-bundle-analyzer": "^3.4.1",
    "webpack-cli": "^3.3.7"
  }
}
